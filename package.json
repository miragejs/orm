{
  "name": "@miragejs/orm",
  "version": "0.1.0",
  "description": "ORM features of MirageJS server",
  "keywords": [],
  "license": "MIT",
  "author": "Kyrylo Hrishchenko",
  "main": "./lib/dist/index.js",
  "module": "./lib/dist/index.mjs",
  "types": "./lib/dist/index.d.ts",
  "type": "module",
  "scripts": {
    "format": "prettier --check \"lib/src/**/*.ts\"",
    "format:fix": "prettier --write \"lib/src/**/*.ts\"",
    "lint": "eslint lib/src --ext .ts",
    "lint:fix": "pnpm lint --fix",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:dev": "vitest",
    "test:types": "vitest run --typecheck",
    "check-all": "pnpm format && pnpm lint && pnpm typecheck",
    "fix-all": "pnpm format:fix && pnpm lint:fix",
    "test-all": "pnpm test:types && pnpm test",
    "build": "tsup lib/src/index.ts --dts --out-dir lib/dist",
    "build:prod": "pnpm check-all && pnpm test-all && pnpm build"
  },
  "devDependencies": {
    "@eslint/js": "^9.27.0",
    "@eslint/json": "^0.12.0",
    "@types/node": "^22.15.21",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import-x": "^4.6.3",
    "eslint-plugin-jsdoc": "^50.6.17",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-unused-imports": "^4.3.0",
    "globals": "^16.1.0",
    "prettier": "^3.5.3",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.1",
    "vitest": "^3.1.4"
  }
}