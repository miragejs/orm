{
  "name": "task-management-dashboard",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "Example Task Management Dashboard using miragejs-orm",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
